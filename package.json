{
  "name": "wutthelint",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "eslint src",
    "test:visual": "jest",
    "test:size": "bundlesize",
    "start": "npm run -s serve",
    "build": "preact build --template src/index.html -p",
    "serve": "preact serve",
    "deploy": "npm run build && npm run test:size && cp now.json build/now.json && cd build && now && now alias",
    "dev": "preact watch"
  },
  "alias": "wuththelint",
  "keywords": [],
  "author": "Sara Vieira",
  "license": "MIT",
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "bundlesize": [
    {
      "path": "./build/bundle.*.js",
      "maxSize": "24 kB"
    },
    {
      "path": "./build/route-*.js",
      "maxSize": "35 kB"
    },
    {
      "path": "./build/sw.js",
      "maxSize": "3 kB"
    }
  ],
  "devDependencies": {
    "bundlesize": "^0.16.0",
    "eslint": "^4.3.0",
    "eslint-config-synacor": "^2.0.0",
    "if-env": "^1.0.0",
    "jest": "^22.0.6",
    "jest-image-snapshot": "^1.0.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "navalia": "^1.2.0",
    "npm-run-all": "^4.0.2",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "animate-css-styled-components": "0.0.20",
    "axios": "^0.17.0",
    "capitalize-it": "^1.2.1",
    "preact": "^8.2.1",
    "preact-compat": "^3.16.0",
    "preact-router": "^2.5.5",
    "styled-components": "^2.1.1"
  }
}
